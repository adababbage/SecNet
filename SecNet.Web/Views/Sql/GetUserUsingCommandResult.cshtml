@model IEnumerable<UserDetailViewModel>
@{
    ViewData["Title"] = "User Search Result";
}

<h1>User Search Results</h1>
<div>
    <table>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
        </tr>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
            </tr>
        }
    </table>
</div>
